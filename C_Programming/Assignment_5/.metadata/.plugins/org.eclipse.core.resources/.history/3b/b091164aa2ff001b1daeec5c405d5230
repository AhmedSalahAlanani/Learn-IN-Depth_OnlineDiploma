#include <stdio.h>

struct Sstudent
{
	char name[100];
	int roll;
	float mark;
};

int main (void)
{
	struct Sstudent student_1;

	printf("Enter information of students: \r\n");

	printf("Enter name: ");
	fflush(stdin);	fflush(stdout);
	scanf("%s",student_1.name);

	printf("Enter name: ");
	fflush(stdin);	fflush(stdout);
	scanf("%s",student_1.name);

	return 0;
}
